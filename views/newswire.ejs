<%- include("includes/head.ejs") %>
</head>

<body>
    <%- include("includes/navbar.ejs") %>

    <h1>News</h1>
    <div id="news-list">
        <% newsItems.forEach(news => { %>
            <div>
                <h2><%= news.name %></h2>
                <p><%= news.content %></p>
                <img src="<%= news.image %>" alt="<%= news.name %> image">
                <p>Published Date: <%= new Date(news.publishedDate).toDateString() %></p>
            </div>
        <% }) %>
    </div>
    